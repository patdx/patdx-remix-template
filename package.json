{
  "name": "patdx-remix-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "wrangler pages deploy",
    "dev": "remix vite:dev",
    "start": "wrangler pages dev",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "update-react": "pnpm install --save-exact react@rc react-dom@rc",
    "lint": "biome lint",
    "lint:fix": "biome lint --fix",
    "format": "biome format --write"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.11.0",
    "@remix-run/cloudflare-pages": "2.11.0",
    "@remix-run/react": "2.11.0",
    "isbot": "5.1.13",
    "react": "rc",
    "react-dom": "rc"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@cloudflare/workers-types": "4.20241205.0",
    "@remix-run/dev": "2.11.0",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "autoprefixer": "10.4.20",
    "postcss": "8.4.40",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4",
    "unplugin-auto-import": "0.18.2",
    "vite": "5.3.5",
    "vite-tsconfig-paths": "4.3.2",
    "wrangler": "3.95.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [],
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom"
    }
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
